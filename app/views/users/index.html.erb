<div class="page-header">
  <h1>
    All Users
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
      <div class="list-group">
        <% @users.each do |user| %>
        <a href="/users/<%= user.id %>">
        <button type="button" class="list-group-item"><%= user.username %></button>
        </a>
        <% end %>
      </div>
  </div>
</div>
